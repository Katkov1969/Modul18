k = int(input('Введите количество запрашиваемых билетов: '))
Count = 0 # Начальная сумма стоимости билетов
C1 = 0    # Цена за билет лица моложе 18 лет
C2 = 990  # Цена за билеты лицам от 18 до 25 лет
C3 = 1390 # Цена за билеты лицам старше 25 лет
for i in range(1,k+1):
    v = int(input('Введите возраст посетителя: '))
    if v < 18:
        Count = Count + C1
    elif 18 <= v < 25:
        Count = Count + C2
    else:
        Count = Count + C3
print("Общая стоимость билетов без скидки ", Count)
if k > 3:
    Count1 = 9 * Count / 10
    print("Стоимость билетов со скидкой ", Count1)
else:
    print("Скидка на билеты не положена")
